			<data-type>String</data-type>
			<default-value></default-value>
			<use-dictionary-value>True</use-dictionary-value>
		</attribute>
		<attribute>
			<attribute-ids>0:18</attribute-ids>
			<packet-type>1</packet-type>
			<db-field>REPLY_MESSAGE</db-field>
			<data-type>String</data-type>
			<default-value></default-value>
			<use-dictionary-value>True</use-dictionary-value>
		</attribute>
	</db-field-mapping>
</user-statistic-post-auth-plugin>

