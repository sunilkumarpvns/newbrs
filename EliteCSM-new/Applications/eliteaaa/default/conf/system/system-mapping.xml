<?xml version="1.0" encoding="UTF-8"?>
	<!-- <!DOCTYPE service-mapping SYSTEM "http://www.elitecore.com">-->


	<!-- ======================================================= -->
	<!-- Mapping of services with actual implementation classes. -->
	<!-- ======================================================= -->

	<!--
		IMPORTANT | Please do not edit this file if you do not have the exact
		idea about | each element and the implications of modification.
		Imporper configuration | of this file may lead to service failure.
	-->
<mapping>
	<rad-plugins>
		<plugin>
			<plugin-name>UNIVERSAL_AUTH_PLUGIN</plugin-name>
			<class-name>com.elitecore.aaa.radius.service.auth.plugins.UniversalAuthPlugin</class-name>
			<conf-class-name>com.elitecore.aaa.radius.service.auth.plugins.conf.impl.UniversalAuthPluginCofigurable</conf-class-name>
			<description>Applies universal pre and post auth policies
			</description>
		</plugin>
		<plugin>
			<plugin-name>UNIVERSAL_ACCT_PLUGIN</plugin-name>
			<class-name>com.elitecore.aaa.radius.service.acct.plugins.UniversalAcctPlugin</class-name>
			<conf-class-name>com.elitecore.aaa.radius.service.acct.plugins.conf.impl.UniversalAcctPluginConfigurable</conf-class-name>
			<description>Applies universal pre and post acct policies</description>
		</plugin>
		<plugin>
			<plugin-name>QUOTA_MANAGER_PLUGIN</plugin-name>
			<class-name>com.elitecore.aaa.radius.plugins.quotamgr.QuotaManagerPlugin</class-name>
			<conf-class-name>com.elitecore.aaa.radius.plugins.quotamgr.conf.impl.QuotaManagerPluginConfigurable</conf-class-name>
			<description>This Plugin used to simulate the NAS reauth request.</description>
		</plugin>
		<plugin>
			<plugin-name>ZERO_SESSION_TIME_FILTER_PLUGIN</plugin-name>
			<class-name>com.elitecore.aaa.radius.plugins.zstfilter.ZeroSessionTimeFilterPlugin</class-name>
			<conf-class-name></conf-class-name>
			<description>Skips processing of packets with Acct-Session-Time attribute with value less than 0</description>
		</plugin>
		<plugin>
			<plugin-name>STRING_OPERATION_PLUGIN</plugin-name>
			<class-name>com.elitecore.aaa.radius.plugins.strop.StringOperationPlugin</class-name>
			<conf-class-name>com.elitecore.aaa.radius.plugins.strop.conf.impl.StringOperationPluginConfigurable</conf-class-name>
			<description>Does String Operations on Attributes of the packets</description>
		</plugin>
		<plugin>
			<plugin-name>USER_STATISTIC_POST_AUTH_PLUGIN</plugin-name>
			<class-name>com.elitecore.aaa.radius.plugins.userstatistic.UserStatisticPostAuthPlugin</class-name>
			<conf-class-name>com.elitecore.aaa.radius.plugins.userstatistic.conf.impl.UserStatisticAuthPluginConfigurable</conf-class-name>
			<description>USER_STATISTIC_POST_AUTH_PLUGIN</description>
		</plugin>
		<plugin>
			<plugin-name>QUOTA_MANAGEMENT_PLUGIN</plugin-name>
			<class-name>com.elitecore.aaa.radius.plugins.quotamgmt.QuotaManagementPlugin</class-name>
			<conf-class-name>com.elitecore.aaa.radius.plugins.quotamgmt.conf.impl.QuotaManagementPluginConfigurable</conf-class-name>
			<description>Does the Volume based and Duration based quota management</description>
		</plugin>
		<plugin>
			<plugin-name>PROXY_DECISION_PLUGIN</plugin-name>
			<class-name>com.elitecore.aaa.radius.plugins.proxy.ProxyDecisionPlugin</class-name>
			<conf-class-name>com.elitecore.aaa.radius.plugins.proxy.conf.impl.ProxyDecisionPluginConfigurable</conf-class-name>
			<description>Proxies if the user is not found</description>
		</plugin>
	</rad-plugins>
	<diameter-plugins>
	<plugin>
			<plugin-name>UNIVERSAL_DIAMETER_PLUGIN</plugin-name>
			<class-name>com.elitecore.diameterapi.plugins.universal.UniversalDiameterPlugin</class-name>
			<conf-class-name>com.elitecore.diameterapi.plugins.universal.conf.impl.UniversalDiameterPluginConfigurable</conf-class-name>
			<description>Applies universal in and out policies</description>
		</plugin>
	</diameter-plugins>
</mapping>

	<!-- ======================================================= -->
	<!-- End of Mapping services.                                -->
	<!-- ======================================================= -->



