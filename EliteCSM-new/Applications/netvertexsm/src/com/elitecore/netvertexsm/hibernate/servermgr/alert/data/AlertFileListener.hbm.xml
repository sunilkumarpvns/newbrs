<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping>

	<class name="com.elitecore.netvertexsm.datamanager.servermgr.alert.data.AlertFileListenerData" table="TBLMFILELISTENER" >          

		<id name="fileListenerId" type="java.lang.Long" column="FILELISTNERID">
		    <generator
				class="com.elitecore.netvertexsm.hibernate.core.system.util.EliteSequenceGenerator">				
			</generator>		
		</id>
		<property name="fileName" type="string" column="FILENAME" not-null="false"/>
		<property name="listenerId" type="java.lang.Long" column="INSTANCEID" not-null="false"/>
		<property name="rollingType" type="java.lang.Long" column="ROLLINGTYPE" not-null="false"/>
		<property name="rollingUnit" type="java.lang.Long" column="ROLLINGUNIT" not-null="false"/>
		<property name="maxRollingUnit" type="java.lang.Long" column="MAXROLLINGUNIT" not-null="false"/>
		<property name="compRollingUnit" type="string" column="COMPROLLINGUNIT" not-null="false"/>
	</class>
	
</hibernate-mapping>

          
