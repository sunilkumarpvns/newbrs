<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping>

	<class name="com.elitecore.netvertexsm.datamanager.servermgr.dictionary.data.DataTypeData" table="TBLSDATATYPE" >          
	 
		<id name="dataTypeId" type="string"  >
		  <column name="DATATYPEID" sql-type="CHAR(5)" not-null="true"/>
		</id>
     
		<property name="alias" type="string">
		  <column name="ALIAS" sql-type="VARCHAR2(25)" not-null="true"/>
		</property>

		<property name="name" type="string">
		  <column name="NAME" sql-type="VARCHAR2(50)" not-null="true"/>
		</property>
     
		<property name="description" type="string">
		  <column name="DESCRIPTION" sql-type="VARCHAR2(255)" not-null="false"/>
		</property>

		<property name="systemGenerated" type="string">
		  <column name="SYSTEMGENERATED" sql-type="CHAR(1)" not-null="true"/>
		</property>

		<property name="javaScriptRegex" type="string">
		  <column name="JAVASCRIPTREGEX" sql-type="VARCHAR2(255)" not-null="false"/>
		</property>

		<property name="javaRegex" type="string">
		  <column name="JAVAREGEX" sql-type="VARCHAR2(255)" not-null="false"/>
		</property>

		<property name="errorMessage" type="string">
		  <column name="ERRORMESSAGE" sql-type="VARCHAR2(255)" not-null="false"/>
		</property>
		
		<property name="dictionaryType" type="string">
		  <column name="DICTIONARYTYPE" sql-type="CHAR(1)" not-null="true"/>
		</property>

	</class>
	
</hibernate-mapping>

          
