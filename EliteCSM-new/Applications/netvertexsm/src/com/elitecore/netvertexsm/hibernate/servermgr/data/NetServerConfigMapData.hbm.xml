<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping>
	<class name="com.elitecore.netvertexsm.datamanager.servermgr.data.NetServerConfigMapData" table="TBLMNETSERVERCONFIGMAP">
		<composite-id>
			<key-property name="netServerTypeId" column="NETSERVERTYPEID" type="string"/>
			<key-property name="netConfigId" column="NETCONFIGID" type="string"/>
		</composite-id>
		<property name="netConfigMapTypeId" type="string">
			<column name="NETCONFIGMAPTYPEID" sql-type="CHAR(1)" not-null="true"></column>
		</property>
	</class>
</hibernate-mapping>          
