<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.2//EN" "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
    
<validators>
    
	<field name="changePasswordData.oldPassword">
		<field-validator type="requiredstring">
			<message>Old Password Missing</message>
		</field-validator>
	</field>
	
	<field name="changePasswordData.newPassword">
		<field-validator type="requiredstring">
			<message>New Password Missing</message>
		</field-validator>
	</field>
</validators>

