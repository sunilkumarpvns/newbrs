<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping>

	<class name="com.elitecore.elitesm.datamanager.radius.clientprofile.data.VendorData" table="TBLMVENDOR" >          

		<id name="vendorInstanceId" type="StringType" column="VENDORINSTANCEID">
			<generator class="com.elitecore.elitesm.hibernate.core.system.util.UUIDGenerator"></generator>
		</id>
		<property name="vendorId" type="long" column="VENDORID" not-null="true"/>
		<property name="vendorName" type="StringType" column="VENDORNAME" not-null="true"/>
 	</class>
	
</hibernate-mapping>

          
