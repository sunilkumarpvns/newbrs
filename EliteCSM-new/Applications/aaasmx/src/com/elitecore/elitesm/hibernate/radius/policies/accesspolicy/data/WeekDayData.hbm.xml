<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.elitecore.elitesm.datamanager.radius.policies.accesspolicy.data.WeekDayData" table="TBLSWEEKDAY">
		<id name="weekDayId"  type="CharType">
			<column name="WEEKDAYID" sql-type="CHAR(3)" not-null="true"/>
		</id>
		<property name="alias" type="StringType">
			<column name="ALIAS" sql-type="VARCHAR(25)" not-null="true"/>
		</property>
		<property name="name" type="StringType">
			<column name="NAME" sql-type="VARCHAR(50)" not-null="true"/>
		</property>
		<property name="description" type="StringType">
			<column name="DESCRIPTION" sql-type="VARCHAR(255)" not-null="true"></column>
		</property>
		<property name="systemGenerated" type="CharType">
			<column name="SYSTEMGENERATED" sql-type="CHAR(1)" not-null="true"></column>
		</property>
	</class>
</hibernate-mapping>