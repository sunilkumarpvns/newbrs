<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping                                            
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"                    
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping>
	<class name="com.elitecore.elitesm.datamanager.servermgr.transmapconf.data.TranslationMappingInstDetailRelData" table="TBLMMAPPINGINSTDETAILREL">
	
		<composite-id>
			<key-property name="mappingInstanceId" column="MAPPINGINSTANCEID" type="StringType"></key-property>
			<key-property name="detailId" column="DETAILID" type="StringType"></key-property>
		</composite-id>
			
		<property name="orderNumber" type="java.lang.Long"> 
			<column name="ORDERNUMBER" sql-type="NUMERIC(20)" not-null="false" /> 
		</property>	
	</class>

</hibernate-mapping> 

	

