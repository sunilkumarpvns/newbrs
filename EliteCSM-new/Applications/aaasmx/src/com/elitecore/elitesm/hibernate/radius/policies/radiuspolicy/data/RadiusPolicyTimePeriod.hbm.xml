<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping>
	<class name="com.elitecore.elitesm.datamanager.radius.policies.radiuspolicy.data.RadiusPolicyTimePeriod" table="TBLMRADIUSPOLICYTIMEPERIOD" >
			<id name="timePeriodId" column="TIMEPERIODID" type="StringType">
				<generator class="com.elitecore.elitesm.hibernate.core.system.util.UUIDGenerator"></generator>
			</id>
			
			<property name="monthOfYear" type="StringType">	
				<column name="MONTHOFYEAR" sql-type="VARCHAR(100)" not-null="false"/>	
			</property>
			
			<property name="dayOfMonth" type="StringType">	
				<column name="DAYOFMONTH" sql-type="VARCHAR(100)" not-null="false"/>	
			</property>
			
			<property name="dayOfWeek" type="StringType">	
				<column name="DAYOFWEEK" sql-type="VARCHAR(100)" not-null="false"/>	
			</property>
			
			<property name="timePeriod" type="StringType">	
				<column name="TIMEPERIOD" sql-type="VARCHAR(100)" not-null="false"/>	
			</property>
			
			<property name="radiusPolicyId" type="StringType">	
				<column name="RADIUSPOLICYID" sql-type="VARCHAR(36)" not-null="true"/>	
			</property>
			
			<property name="orderNumber" type="java.lang.Integer" column="ORDERNUMBER" not-null="false" /> 
			
		</class>
</hibernate-mapping>          