<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping                                            
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"                    
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping>
	<class name="com.elitecore.elitesm.datamanager.servermgr.transmapconf.data.TranslatorTypeData" table="TBLSTRANSLATORTYPE">

		<id name="translatorTypeId" column="TRANSLATORTYPEID" type="CharType"></id> 

		<property name="name" type="StringType"> 
			<column name="NAME" sql-type="VARCHAR(100)" not-null="false" /> 
		</property>

		<property name="value" type="StringType"> 
			<column name="VALUE" sql-type="VARCHAR(100)" not-null="false" /> 
		</property>
		
		<property name="translateTo" type="CharType"> 
			<column name="TRANSLATETO" sql-type="CHAR(1)" not-null="false" /> 
		</property>
		
		<property name="translateFrom" type="CharType"> 
			<column name="TRANSLATEFROM" sql-type="CHAR(1)" not-null="false" /> 
		</property>
		
	</class>

</hibernate-mapping> 

	

