<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<driver-instance>
    <name>def_RadiusClassicCSVDriver</name>
    <description>This driver is created by EliteAAA Default configuration Setup. This will generated the CDR with daily rolling without header</description>
    <classic-csv-acct-driver>
        <header>false</header>
        <delimeter>,</delimeter>
        <multivalue-delimeter>;</multivalue-delimeter>
        <enclosing-character>"</enclosing-character>
        <cdr-timestamp-format>EEE dd MMM yyyy hh:mm:ss aaa</cdr-timestamp-format>
        <file-name>CDRs.csv</file-name>
        <location>data/csvfiles</location>
        <create-blank-file>true</create-blank-file>
        <prefix-file-name>0:1</prefix-file-name>
        <default-folder-name>no_nas_ip_address</default-folder-name>
        <folder-name>0:1</folder-name>
        <allocating-protocol>NONE</allocating-protocol>
        <address>127.0.0.1:8080</address>
        <destination-location>255.255.255.0</destination-location>
        <username>admin</username>
        <post-operation>archive</post-operation>
        <archive-locations>data/csvfiles/archive</archive-locations>
        <failover-time>3</failover-time>
        <time-boundry>Daily</time-boundry>
        <size-based-rolling-unit>1</size-based-rolling-unit>
        <time-based-rolling-unit>4096</time-based-rolling-unit>
        <record-based-rolling-unit>0</record-based-rolling-unit>
        <use-dictionary-value>false</use-dictionary-value>
        <sequence-range>[001-100]</sequence-range>
        <sequence-position>suffix</sequence-position>
        <sequence-globalization>false</sequence-globalization>
        <cdr-timestamp-header>CDRTimeStamp</cdr-timestamp-header>
        <cdr-timestamp-position>SUFFIX</cdr-timestamp-position>
        <classic-csv-field-mappings>
            <classic-csv-field-mapping>
                <header>Acct-Session-ID</header>
                <attribute-id>0:44</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Acct-Status-Type</header>
                <attribute-id>0:40</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Acct-Input-Gigawards</header>
                <attribute-id>0:52</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>NAS-Port</header>
                <attribute-id>0:5</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Event-Timestamp</header>
                <attribute-id>0:55</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Acct-Multi-Session-ID</header>
                <attribute-id>0:50</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Acct-Input-Packets</header>
                <attribute-id>0:47</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Acct-Delay-Time</header>
                <attribute-id>0:41</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Acct-Output-Gigawords</header>
                <attribute-id>0:53</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Acct-Output-Octets</header>
                <attribute-id>0:43</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>NAS-Port-ID</header>
                <attribute-id>0:87</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>NAS-IP-Address</header>
                <attribute-id>0:4</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>NAS-Identifier</header>
                <attribute-id>0:32</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Calling-Station-ID</header>
                <attribute-id>0:31</attribute-id>
                <default-value>AB:1C:DE:3F:32:6F</default-value>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>UserName</header>
                <attribute-id>0:1</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Acct-Session-Time </header>
                <attribute-id>0:46</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Acct-Input-Octets</header>
                <attribute-id>0:42</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
            <classic-csv-field-mapping>
                <header>Acct-Output-Packets</header>
                <attribute-id>0:48</attribute-id>
                <use-dictionary-value>false</use-dictionary-value>
            </classic-csv-field-mapping>
        </classic-csv-field-mappings>
        <classic-csv-strip-pattern-relation-mappings>
            <classic-csv-strip-pattern-relation-mapping>
                <attribute-id>0:1</attribute-id>
                <pattern>suffix</pattern>
                <separator>-</separator>
            </classic-csv-strip-pattern-relation-mapping>
        </classic-csv-strip-pattern-relation-mappings>
    </classic-csv-acct-driver>
</driver-instance>