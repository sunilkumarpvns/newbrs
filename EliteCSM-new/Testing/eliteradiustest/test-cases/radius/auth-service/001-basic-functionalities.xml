<aaa-test-cases>
<!-- ================================= TEST-CASE-1 ========================================== -->	
	<test-case>
		<id>TEST-CASE-1</id>
		<description>Test authentication with correct password - without any check or reject items</description>
		<request-packet>
		  <code>01</code>
  		  <identifier>01</identifier>
		  <attributes>
		    <attribute>
		      <id>01</id>
		      <type>string</type>
		      <value>authonlyuser</value>
		    </attribute>
		    <attribute>
		      <id>02</id>
		      <type>string</type>
		      <value>authpassword</value>
		    </attribute>
		    <attribute>
		      <id>32</id> <!-- Nas-Identifier -->
		      <type>string</type>
		      <value>Elitecore-Test-NAS</value>
		    </attribute>
	  	</attributes>
		</request-packet>

		<expected-response-packet>
		   <code>02</code>
 		   <identifier>01</identifier>
		   <attributes>	
    		 <attribute>
		       <id>27</id>
		       <type>integer</type>
		       <value>600</value>
		     </attribute>
		     <attribute>
      		   <id>18</id>
		       <type>string</type>
		       <value>Authentication success</value>
		     </attribute>		    		     
		   </attributes>	
	    </expected-response-packet>
	</test-case>
