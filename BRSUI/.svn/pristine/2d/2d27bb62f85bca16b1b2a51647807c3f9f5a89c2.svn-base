<html>
<head>
 <link rel="stylesheet" type="text/css" href="../css/bootstrap.css">
  <link rel="stylesheet" type="text/css" href="../css/login.css">
<script src="../js/angular.js" type="text/javascript"></script>
<script src="../js/loginForm.js" type="text/javascript"></script>

 </head>
<body>
<div class="container" ng-app="BRS_Login" style="margin-left:520px; margin-right:50px " >
	<div class="panel panel-primary"  ng-controller="Passenger_Login_Controller" >
		<div class="panel-heading">
			<h3 class="panel-title">Passenger Login</h3>
		</div>
		<div class="panel-body">
			<div class="form-group">
				<label for="userName">userName</label> <input type="text"
					class="form-control" ng-model="userName">
			</div>

			<div class="form-group">
				<label for="Password">password</label> <input type="password"
					class="form-control" ng-model="password">
			</div>

			<div class="form-group">
				<label for="Role">role</label> <input type="text"
					class="form-control" ng-model="role">
				 <input type="hidden"
					class="form-control" ng-model="ipAddress">
					<input type="hidden"
					class="form-control" ng-model="sessionId" />
					
			</div>
			
		<div class="form-group">
			<button class="btn btn-primary" value="Login" ng-click="login()">Login</button>

		</div>

	</div>

</div>
</body>

</html>

